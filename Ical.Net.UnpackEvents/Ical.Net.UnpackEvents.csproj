<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Title>Ical.Net.UnpackEvents</Title>
    <PackageProjectUrl>https://github.com/42ama/ical.net.unpackevents</PackageProjectUrl>
    <RepositoryUrl>https://github.com/42ama/ical.net.unpackevents</RepositoryUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Ical.Net.UnpackEvents</PackageId>
    <Version>2.0.5</Version>
    <Authors>Maxim Alonov</Authors>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Ical.Net extension for unpacking recurring events into a plain collection. Allows to view different instances of events as distinct objects.</Description>
    <PackageReadmeFile>README.md</PackageReadmeFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Ical.Net" Version="4.2.0" />
    <None Include="README.md" Pack="true" PackagePath="\"/>
  </ItemGroup>

</Project>
